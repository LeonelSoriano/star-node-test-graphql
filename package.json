{
  "_comment": "chromium chrome://inspect/#devices*/",
  "name": "your-project-name",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "server": "babel-node  ./src/bin/www ",
    "server:prod": "babel-node  ./dist/bin/www",
    "server:debug": "babel-node --inspect --inspect-brk  ./dist/bin/www",
    "dev": "NODE_ENV=development npm-run-all server",
    "clean": "rimraf dist",
    "build": "babel ./src -d dist --source-maps --require babel-polyfill",
    "prod": "NODE_ENV=production npm-run-all clean build server:prod",
    "debug": "NODE_ENV=production npm-run-all clean build server:debug",
    "watch": "nodemon",
    "doc": "./node_modules/jsdoc/jsdoc.js -c jsdoc.conf.json",
    "test": "./node_modules/.bin/mocha --require @babel/register test/unit test/int",
    "test:unit": "./node_modules/.bin/mocha --timeout 15000 --require @babel/register --require babel-polyfill test/unit/**/*js    test/unit/*js test/unit/**/**/*js ",
    "test:int": "./node_modules/.bin/mocha --require @babel/register test/int",
    "test:debug": "./node_modules/.bin/mocha --inspect-brk --require @babel/register test/unit/**/*js test/unit/*js "
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "babel-polyfill": "^6.26.0",
    "cache-manager": "^2.9.0",
    "cache-manager-redis": "^0.6.0",
    "chai-as-promised": "^7.1.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^7.0.0",
    "express": "~4.16.0",
    "graphql-yoga": "^1.17.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.2",
    "morgan": "~1.9.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.2",
    "sinon-mongoose": "^2.3.0",
    "validator": "^10.11.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.4",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "chai": "^4.2.0",
    "jsdoc": "^3.5.5",
    "mocha": "^6.1.4",
    "nodemon": "^1.18.11"
  }
}
